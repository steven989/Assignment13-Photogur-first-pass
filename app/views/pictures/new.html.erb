<h1>Add a picture</h1>
<%= form_tag "/pictures" do %>

    <p> 
        <%= label_tag "Name" %> <br>
        <%= text_field_tag "name" %> 

    </p>

    <p>
        <%= label_tag "Year" %> <br>
        <%= text_field_tag "year" %>
    </p>

    <p>
        <%= label_tag "Artist" %> <br>
        <%= text_field_tag "artist" %>
    </p>

    <p>
        <%= label_tag "URL" %> <br>
        <%= text_field_tag "url" %>
    </p>

    <p>
        <%= submit_tag "Save" %>
    </p>

<% end %>